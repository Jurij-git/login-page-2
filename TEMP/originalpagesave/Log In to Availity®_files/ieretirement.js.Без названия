$(function(){if($.browser.msie){if(isIECompatMode()){displayCompatModal();return}if($.browser.version<11){displayIERetiredModal();return}}if(isUnsupportedBrowser()){return displayDismissableUnsupportedBrowserModal()}});function calcNumOfDays(){var Today=new Date();var novRelease=new Date();novRelease.setDate(15);novRelease.setMonth(10);novRelease.setYear(2014);var datediff=novRelease.getTime()-Today.getTime();var numDays=datediff/(24*60*60*1000);return Math.round(numDays)}function doLater(){createCookie("displayed","yes",1);$("#IERetirementModal").dialog("close")}function closeUnsupportedBrowserModal(){$("#DismissableUnsupportedBrowserModal").dialog("close")}function displayCompatPage(){logToSplunk("IE Modal Button Click","View Compatibility Mode Instructions");window.location.href="https://apps.availity.com/availity/Demos/Compatibility_View/Turning_off_Compatibility_View.htm"}function displayIE8RetirementMessage(){$("#ie8-retirement-message").show()}function displayCompatMessage(){$("#ie-compat-message").show()}function isUnsupportedBrowser(){return isMSEdge()}function isMSEdge(){return navigator.userAgent.indexOf("Edge")>-1}function isIECompatMode(){var isIE=false;var trueVersion=0;var actingVersion=0;var compatibilityMode=false;var trident=navigator.userAgent.match(/Trident\/(\d+)/);if(trident){isIE=true;trueVersion=parseInt(trident[1],10)+4}var msie=navigator.userAgent.match(/MSIE (\d+)/);if(msie){isIE=true;actingVersion=parseInt(msie[1])}else{actingVersion=trueVersion}if(isIE&&trueVersion>0&&actingVersion>0){compatibilityMode=trueVersion!=actingVersion}var newerVersion=navigator.userAgent.indexOf("Trident/6")>-1||navigator.userAgent.indexOf("Trident/7")>-1;if(newerVersion){$("#doLaterButton").show()}return compatibilityMode}function displayCompatModal(){$("#IECompatibilityMode").dialog({modal:true,dialogClass:"ui-dialog-osx",draggable:false,resizable:false,position:["center","middle"],show:"blind",hide:"blind",width:455,closeOnEscape:false})}function displayIERetiredModal(){$("#IERetiredModal").dialog({modal:true,dialogClass:"ui-dialog-osx",draggable:false,resizable:false,position:["center","middle"],show:"blind",hide:"blind",width:455,closeOnEscape:false})}function displayIERetiringModal(){$("#IERetirementModal").dialog({modal:true,dialogClass:"ui-dialog-osx",draggable:false,resizable:false,position:["center","middle"],show:"blind",hide:"blind",width:455})}function displayDismissableUnsupportedBrowserModal(){$("#DismissableUnsupportedBrowserModal").dialog({modal:true,dialogClass:"ui-dialog-osx",draggable:false,resizable:false,position:["center","middle"],show:"blind",hide:"blind",width:455})}function chromeClick(){logToSplunk("IE Modal Button Click","Upgrade to Chrome",function(){window.location.href="http://www.google.com/chrome/"})}function firefoxClick(){logToSplunk("IE Modal Button Click","Upgrade to Firefox",function(){window.location.href="https://www.mozilla.org/en-US/firefox/new/"})}function ieClick(){logToSplunk("IE Modal Button Click","Upgrade to a newer IE",function(){window.location.href="http://windows.microsoft.com/en-US/internet-explorer/download-ie"})}function logToSplunk(location,action,callback){if(callback!==undefined){callback()}}function createCookie(name,value,days){var expires;if(days){var date=new Date();date.setTime(date.getTime()+(days*24*60*60*1000));expires="; expires="+date.toGMTString()}else{expires=""}document.cookie=name+"="+value+expires+"; path=/"}function getCookie(name){var nameEQ=name+"=";var ca=document.cookie.split(";");for(var i=0;i<ca.length;i++){var c=ca[i];while(c.charAt(0)==" "){c=c.substring(1,c.length)}if(c.indexOf(nameEQ)==0){return c.substring(nameEQ.length,c.length)}}return null}function eraseCookie(name){createCookie(name,"",-1)}function getRandomInt(min,max){var randomNum=Math.floor(Math.random()*(max-min+1))+min;return randomNum};